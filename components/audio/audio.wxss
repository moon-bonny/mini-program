.progress-wrap slider {
  display: inline-block;
  width: 100%;
}
.progress-wrap{
  position: relative;
  width: 100%;
  min-height: 295rpx;
  margin-bottom: 15rpx;
  border-bottom: 1rpx solid #eaeaea;
}
.play-btn image{
  width: 120rpx;
  height: 120rpx;
}
.history image{
  width: 40rpx;
  height: 40rpx;
}
.progress-wrap .time{
  padding-top: 11rpx;
  height: 74rpx;
  line-height: 63rpx;
  font-size: 24rpx;
  color: #999;
}
.progress-wrap .time .over-time{
  float: left;
}
.progress-wrap .time .total-time{
  float: right;
}
.progress-wrap slider {
  margin-left: 0;
  margin-bottom: 10rpx;
  width: 100%;
}
.progress-wrap .play-btn{
  margin-top: 20rpx;
}
.history{
  position: absolute;
  left: 555rpx;
  top:180rpx;
}

.play-history-wrap.trans-y {
  transform: translate(0, 86rpx);
}
.play-history-wrap{
  position: fixed;
  right: 20rpx;
  bottom: 112rpx;
  width: 128rpx;
  height: 128rpx;
  z-index: 555;
  background: url(data:image/png;base64,) no-repeat;
  background-size: cover;
}
.play-history-wrap image{
  position: absolute;
  left: calc(50% - 22rpx);
  top:calc(50% - 22rpx);
  width: 44rpx;
  height: 44rpx;
}