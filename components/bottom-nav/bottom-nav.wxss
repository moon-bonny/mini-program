.bottom-nav-wrap{
  position: fixed;
  bottom: 0;
  z-index: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100rpx;
  background: #fff;
  border-top: 1px solid #eaeaea;
}
.bottom-nav-item{
  flex: 1;
  display: inline-block;
  text-align: center;
}
.bottom-nav-icon {
  width: 38rpx;
  height: 38rpx;
  margin-bottom: 10rpx;
}
.bottom-nav-txt {
  display: block;
  font-size: 20rpx;
  color: #666;
}